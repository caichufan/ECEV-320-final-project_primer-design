  cpp_18bp_pass_GC_and_Tm_threshold <- subset(cpp_18bp_pass_GC_threshold,(cpp_18bp_pass_GC_threshold$`Tm`)>=50 & (cpp_18bp_pass_GC_threshold$`Tm`)<=60)
}

